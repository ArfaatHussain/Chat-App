{
  "expo": {
    "name": "Chat-App",
    "slug": "Chat-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ChatApp"
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "e2a351ea-6b5c-4f54-a40d-7d91c7b66c10"
      }
    },
    "owner": "arfaat_hussain",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-dev-client"
    ]
  }
}
